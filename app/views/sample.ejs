<%- include('header') %>
<%- include('navbar') %>

<div class="container">
  <div class=container>
    <h1 class="display-4">Sample Data</h1>
    <table class="table table-striped table-dark">
      <thead>
        <tr>
          <th scope="col">id</th>
          <% for (let i = 1; i<18; i++ ) { %> 
            <th scope="col">Q<%- i %></th>
        <% } %>
        </tr>
      </thead>
      <tbody>
        <% data_sets.forEach(entry => { %>
          <tr>
              <th scope="row"><%- entry.id %></th>
              <td><%- entry.Q1 %></td>
              <td><%- entry.Q2 %></td>
              <td><%- entry.Q3 %></td>
              <td><%- entry.Q4 %></td>
              <td><%- entry.Q5 %></td>
              <td><%- entry.Q6 %></td>
              <td><%- entry.Q7 %></td>
              <td><%- entry.Q8 %></td>
              <td><%- entry.Q9 %></td>
              <td><%- entry.Q10 %></td>
              <td><%- entry.Q11 %></td>
              <td><%- entry.Q12 %></td>
              <td><%- entry.Q13 %></td>
              <td><%- entry.Q14 %></td>
              <td><%- entry.Q15 %></td>
              <td><%- entry.Q16 %></td>
              <td><%- entry.Q17 %></td>
          </tr>

        <% }) %>
        
      </tbody>
    </table>
  </div>
</div>

<%- include('footer') %>
